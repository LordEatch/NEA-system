<?xml version="1.0" encoding="utf-8" ?>
<ViewCell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:models="clr-namespace:NEA_system.Models"
             x:Class="NEA_system.Views.Card_ResistanceSet"
             x:DataType="models:ResistanceSet">
    <HorizontalStackLayout>
        <Entry Placeholder="{Binding Mass}"/>
        <Entry Placeholder="{Binding StrictReps}"/>
        <Entry Placeholder="{Binding CheatedReps}"/>
        <Entry Placeholder="{Binding SetComment}"/>
    </HorizontalStackLayout>
</ViewCell>
